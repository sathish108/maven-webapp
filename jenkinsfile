pipeline {
  agent any
    stages {
        stage("clone scm repo") {
            steps {
                git credentialsId: 'github_credentials', url: 'https://github.com/sathish108/maven-webapp'
            }
        }
        stage("build docker image") {
            steps {
                docker build -t webapp:v1 .
                docker run -itd --name server webapp:v1
            }
        }
    }
}